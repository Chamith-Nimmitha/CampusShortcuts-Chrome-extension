{
    "manifest_version":2,
    "name": "Campus Shortcuts",
    "description":"Quick access to courses",
    "version":"2.0.0",
    "icons":{"128": "icon_128.png"},
    "browser_action":{
        "default_icon": "icon_20.png",
        "default_popup": "popup.html"
    },
    "permissions":["activeTab"],
    "background":{
        "scripts":[
            "./popup.js"
        ]
    }
}